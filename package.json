{
  "name": "react-express-starter",
  "version": "1.0.0",
  "description": "React and Express app boilerplate",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client-install": "cd client && npm install"
  },
  "author": "Lewis Nozaki",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^6.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.13"
  }
}
